{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "title": {
      "$id": "/properties/title",
      "type": "string",
      "title": "The Title Schema ",
      "default": "",
      "examples": [
        "Planning"
      ]
    },
    "slug": {
      "$id": "/properties/slug",
      "type": "string",
      "title": "The Slug Schema ",
      "default": "",
      "examples": [
        "planning"
      ]
    },
    "categories": {
      "$id": "/properties/categories",
      "type": "array",
      "items": {
        "$id": "/properties/categories/items",
        "type": "object",
        "properties": {
          "title": {
            "$id": "/properties/categories/items/properties/title",
            "type": "string",
            "title": "The Title Schema ",
            "default": "",
            "examples": [
              "NDC Targets"
            ]
          },
          "slug": {
            "$id": "/properties/categories/items/properties/slug",
            "type": "string",
            "title": "The Slug Schema ",
            "default": "",
            "examples": [
              "ndc_targets"
            ]
          },
          "optional": {
            "$id": "/properties/categories/items/properties/optional",
            "type": "boolean",
            "title": "The Optional Schema ",
            "default": false,
            "examples": [
              false
            ]
          },
          "order": {
            "$id": "/properties/categories/items/properties/order",
            "type": "integer",
            "title": "The Order Schema ",
            "default": 0,
            "examples": [
              0
            ]
          },
          "active": {
            "$id": "/properties/categories/items/properties/active",
            "type": "boolean",
            "title": "The Active Schema ",
            "default": false,
            "examples": [
              true
            ]
          }
        }
      }
    }
  }
}
