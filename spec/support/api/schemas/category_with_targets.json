{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "title": {
      "$id": "/properties/title",
      "type": "string",
      "title": "The Title Schema ",
      "examples": [
        "NDC Targets"
      ]
    },
    "slug": {
      "$id": "/properties/slug",
      "type": "string",
      "title": "The Slug Schema ",
      "examples": [
        "ndc_targets"
      ]
    },
    "optional": {
      "$id": "/properties/optional",
      "type": "boolean",
      "title": "The Optional Schema ",
      "examples": [
        false
      ]
    },
    "order": {
      "$id": "/properties/order",
      "type": "integer",
      "title": "The Order Schema ",
      "examples": [
        0
      ]
    },
    "active": {
      "$id": "/properties/active",
      "type": "boolean",
      "title": "The Active Schema ",
      "examples": [
        true
      ]
    },
    "targets": {
      "$id": "/properties/targets",
      "type": "array",
      "items": {
        "$id": "/properties/targets/items",
        "type": "object",
        "properties": {
          "title": {
            "$id": "/properties/targets/items/properties/title",
            "type": "string",
            "title": "The Title Schema ",
            "examples": [
              "GHG Target"
            ]
          },
          "summary": {
            "$id": "/properties/targets/items/properties/summary",
            "type": ["string", "null"],
            "title": "The Summary Schema ",
            "examples": [
              null
            ]
          },
          "slug": {
            "$id": "/properties/targets/items/properties/slug",
            "type": "string",
            "title": "The Slug Schema ",
            "examples": [
              "ghg_target"
            ]
          },
          "order": {
            "$id": "/properties/targets/items/properties/order",
            "type": "integer",
            "title": "The Order Schema ",
            "examples": [
              0
            ]
          },
          "year": {
            "$id": "/properties/targets/items/properties/year",
            "type": "integer",
            "title": "The Year Schema ",
            "examples": [
              2018
            ]
          },
          "reported_percentage": {
            "$id": "/properties/targets/items/properties/reported_percentage",
            "type": "integer",
            "title": "The Reported_percentage Schema ",
            "examples": [
              0
            ]
          },
          "updated_at": {
            "$id": "/properties/targets/items/properties/updated_at",
            "type": "string",
            "title": "The Updated_at Schema ",
            "examples": [
              "2018-04-30 12:08:13 +0000"
            ]
          }
        },
        "required": [
          "title",
          "summary",
          "slug",
          "order",
          "year",
          "updated_at"
        ]
      }
    }
  },
  "required": [
    "title",
    "slug",
    "optional",
    "order",
    "active",
    "targets"
  ]
}
