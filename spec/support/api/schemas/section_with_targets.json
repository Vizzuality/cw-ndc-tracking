            "$id": "/properties/categories/items/properties/title",
            "type": "string",
            "title": "The Title Schema ",
            "default": "",
            "examples": [
              "NDC Targets"
            ]
          },
          "slug": {
            "$id": "/properties/categories/items/properties/slug",
